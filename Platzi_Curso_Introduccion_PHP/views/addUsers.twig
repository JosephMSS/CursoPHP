{% extends "layout.twig" %}
{% block content %}
<h1>AddUser</h1>
<div class="alert alert-primary" role="alert">
{{ responsiveMessage }}
</div>
{# se utiliza un tipo especial de encoding cuajndo se quiere subir archivos #}
<form action="../add/user" method='POST' >
